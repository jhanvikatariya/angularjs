<!--Pipes with string, date, slice, currency-->

//APP.COMPONENT.HTML
<h1>Pipes</h1>
<h4>{{name | uppercase}}</h4>
<h4>{{name | titlecase}}</h4>
<h4>{{name | lowercase}}</h4>
<h4>{{today | date:"fullDate"}}</h4>
<h4>{{str | slice:10}}</h4>
<h4>{{val | currency:'USD'}}</h4>
<h4>{{val | currency:'CAD'}}</h4>

//APP.COMPONENT.TS
  name="Reena";
  today=Date.now();
  str="Welcome to Angular";
  val=10;
